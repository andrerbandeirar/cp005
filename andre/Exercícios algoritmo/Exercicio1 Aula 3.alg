algoritmo "NomeNota"
var
   nota1,nota2,nota3,nota4,media:real
   nome:caractere
inicio
   escreval("Digite nota 1")
   leia(nota1)
   escreval("Digite nota 2")
   leia(nota2)
   escreval("Digite nota 3")
   leia(nota3)
   escreval("Digete nota 4")
   leia(nota4)
   escreval("Digete nome")
   leia(nome)
   media<-(nota1+nota2+nota3+nota4)/4
   se nota>= 7
   escolha nota
   caso3

